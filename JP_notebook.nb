Cell[CellGroupData[{

Cell[TextData[{
 "Given a (commutation respecting) sorting of the GST list, one then needs to \
use BCH to map ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["e", "A"], 
       SuperscriptBox["e", "B"]}], "..."}], 
     SuperscriptBox["e", "K"]}], "->", 
    SuperscriptBox["e", 
     RowBox[{"F", "(", 
      RowBox[{"A", ",", "B", ",", "...", ",", "K"}], ")"}]]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ee269f54-65c2-4244-a923-f5e36aa3f999"],
 " with F given by some fixed number of terms in the BCH expansion. \n\
Prod2Sum is the second order BCH expansion of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", "A"], 
    SuperscriptBox["e", "B"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0c98c948-5b8b-4422-8822-59cc14cc54fc"],
 "\nProdToSum is the second order BCH expansion of an arbitrary length \
product of matrices to be exponentiated.\nOnce again, all arguments here \
should be passed as the log of the exponential maps, i.e. the argument to \
Exp.\n\[OpenCurlyDoubleQuote]GetErrorMaps\[CloseCurlyDoubleQuote] takes finds \
the exponential error maps from the sorted GST string map terms and returns \
them in a list"
}], "Subsubsection",
 CellChangeTimes->{{3.875379805455646*^9, 3.875380022853944*^9}, {
  3.8753800540437517`*^9, 
  3.875380132879417*^9}},ExpressionUUID->"bd3e9329-6302-4fc5-88cc-\
5509db0abf62"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "Prod2Sum", "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{"Functionality", " ", "for", " ", "mapping", " ", "exp", 
      RowBox[{"(", "a", ")"}], "exp", 
      RowBox[{"(", "b", ")"}]}], "->", 
     RowBox[{"exp", 
      RowBox[{"(", 
       RowBox[{"f", 
        RowBox[{"(", 
         RowBox[{"a", ",", "b"}], ")"}]}], ")"}]}]}], ",", " ", 
    RowBox[{"where", " ", "f", 
     RowBox[{"(", 
      RowBox[{"a", ",", "b"}], ")"}], " ", "can", " ", "be", " ", "expressed",
      " ", "in", " ", "terms", " ", "of", " ", "nested", " ", "commutators", 
     " ", "of", " ", "A", " ", "and", " ", "B"}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Comm", "[", 
   RowBox[{"A_", ",", "B_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"A", "**", "B"}], "-", 
   RowBox[{"B", "**", "A"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Prod2Sum", "[", 
    RowBox[{"A_", ",", "B_"}], "]"}], ":=", 
   RowBox[{"A", "+", "B", "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"Comm", "[", 
      RowBox[{"A", ",", "B"}], "]"}]}]}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"exp", 
     RowBox[{"(", "A", ")"}], "exp", 
     RowBox[{"(", "B", ")"}], "exp", 
     RowBox[{"(", "C", ")"}]}], "->", 
    RowBox[{
     RowBox[{"Prod2Sum", "[", 
      RowBox[{"C", ",", 
       RowBox[{"Prod2Sum", "[", 
        RowBox[{"A", ",", "B"}], "]"}]}], "]"}], "\[IndentingNewLine]", 
     RowBox[{"i", ".", "e", ".", " ", 
      RowBox[{"ProdToSum", "[", 
       RowBox[{"A", ",", "B", ",", "C"}], "]"}]}]}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ProdToSum", "[", "A__", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"args", " ", "=", " ", 
       RowBox[{"Permute", "[", 
        RowBox[{
         RowBox[{"List", "[", "A", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"2", ",", "1"}], "}"}]}], "]"}]}], "}"}], ",", 
     RowBox[{"Fold", "[", 
      RowBox[{
       RowBox[{"ReverseApplied", "[", "Prod2Sum", "]"}], ",", 
       RowBox[{"args", "[", 
        RowBox[{"[", "1", "]"}], "]"}], ",", 
       RowBox[{"args", "[", 
        RowBox[{"[", 
         RowBox[{"2", ";;", 
          RowBox[{"-", "1"}]}], "]"}], "]"}]}], "]"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ProdToSum", "[", "AList_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"args", " ", "=", " ", 
       RowBox[{"Permute", "[", 
        RowBox[{"AList", ",", 
         RowBox[{"{", 
          RowBox[{"2", ",", "1"}], "}"}]}], "]"}]}], "}"}], ",", 
     RowBox[{"Fold", "[", 
      RowBox[{
       RowBox[{"ReverseApplied", "[", "Prod2Sum", "]"}], ",", 
       RowBox[{"args", "[", 
        RowBox[{"[", "1", "]"}], "]"}], ",", 
       RowBox[{"args", "[", 
        RowBox[{"[", 
         RowBox[{"2", ";;", 
          RowBox[{"-", "1"}]}], "]"}], "]"}]}], "]"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GetErrorMaps", "[", "sortStr_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"errMaps", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"bits", " ", "=", " ", 
       RowBox[{"SortBitList", "[", 
        RowBox[{"lgstBitString", "[", 
         RowBox[{"Length", "[", "sortStr", "]"}], "]"}], "]"}]}]}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"idx", " ", "=", "1"}], ",", 
       RowBox[{"idx", " ", "<=", 
        RowBox[{"Length", "[", "bits", "]"}]}], ",", 
       RowBox[{"idx", "+=", "1"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"bits", "[", 
           RowBox[{"[", "idx", "]"}], "]"}], "==", "1"}], ",", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"errMaps", ",", 
           RowBox[{"sortStr", "[", 
            RowBox[{"[", "idx", "]"}], "]"}]}], "]"}], ",", "Nothing"}], 
        "]"}]}], "]"}], ";", 
     RowBox[{"Return", "[", "errMaps", "]"}]}]}], "]"}]}]}], "Input",
